{
  "name": "env-github",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "deploy:dev": "tsx src/app.ts dev",
    "deploy:dev:dry": "cross-env DRY_RUN=true tsx src/app.ts dev",
    "deploy:dev:apply": "cross-env DRY_RUN=false tsx src/app.ts dev",
    "deploy:dev:apply:delete": "cross-env DRY_RUN=false ALLOW_ENV_DELETE=true tsx src/app.ts dev",
    "deploy:idev": "tsx src/app.ts idev",
    "deploy:idev:dry": "cross-env DRY_RUN=true tsx src/app.ts idev",
    "deploy:idev:apply": "cross-env DRY_RUN=false tsx src/app.ts idev",
    "deploy:idev:apply:delete": "cross-env DRY_RUN=false ALLOW_ENV_DELETE=true tsx src/app.ts idev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cross-env": "^10.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0"
  }
}
